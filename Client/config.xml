<?xml version='1.0' encoding='utf-8'?>
<widget id="com.coman3.Boxt" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Text It</name>
    <description>
    </description>
    <author email="me@coman3.me" href="http://coman3.me">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <preference name="EnableWebGL" value="true" />
    <preference name="StatusBarBackgroundColor" value="#f48f42" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-facebook4" spec="https://github.com/jeduan/cordova-plugin-facebook4">
        <variable name="APP_ID" value="195236404242557" />
        <variable name="APP_NAME" value="Boxt" />
    </plugin>
    <universal-links>
        <host name="textit.coman3.xyz"></host>
</universal-links>
</widget>
